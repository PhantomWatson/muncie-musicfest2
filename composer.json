{
    "name": "ballstatecber/munciemusicfest2",
    "description": "Muncie MusicFest - Developed in CakePHP 3",
    "homepage": "http://munciemusicfest.com",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=5.4.16",
        "cakephp-fr/recaptcha": "~0.4",
        "cakephp/cakephp": "~3.1",
        "cakephp/migrations": "~1.0",
        "cakephp/plugin-installer": "*",
        "mobiledetect/mobiledetectlib": "2.*",
        "twbs/bootstrap": "^3.3",
        "xety/cake3-cookieauth": "1.*",
        "xety/cake3-sluggable": "1.*",
        "imagine/imagine": "^0.6.3",
        "james-heinrich/getid3": "^1.9",
        "league/oauth2-facebook": "~1.0",
        "happyworm/jplayer": "2.*"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "~3.2",
        "cakephp/bake": "~1.1",
        "cakephp/cakephp-codesniffer": "*"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "component-dir": "./webroot/js"
    }
}
